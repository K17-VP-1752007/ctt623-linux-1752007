#!/bin/bash

demsotu()
{
    n=0
    while read line; do
	for wd in $line; do
	    n=$(($n+1))
	done
    done
    echo "So tu : $n"
}
lietke()
{
    for i in $*; do
	echo" $i"
    done
}
demsolan()
{
    dem=0
    a="c"
    for i in $*; do
	if [[ $i == $a ]]; then
	    dem=$(($dem+1))
	fi
    done
    echo "So lan lap: $dem"
}
demsotu test


